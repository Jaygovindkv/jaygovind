pipeline {

agent any

stages {

stage(‘Checkout’) {

steps {

checkout([$class: ‘GitSCM’, branches: [[name: ‘*/main’]], extensions: [], userRemoteConfigs: [[url: ‘https://github.com/Jaygovindkv/jaygovind.git‘]]])

}

}

stage (“terraform_init”) {

steps {

sh "terraform init"

}

}

stage (“terraform_Action”) {

steps {

sh "terraform apply –auto-approve"

}

}

}

}
